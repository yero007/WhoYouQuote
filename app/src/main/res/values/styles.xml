<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="windowActionBar">false</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="elevation">0dp</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowActionModeOverlay">true</item>
        <item name="colorPrimary">@color/primaryColor</item>
        <!-- colorPrimaryDark is used for the status bar -->
        <item name="colorPrimaryDark">@color/primaryColorDark</item>

        <!-- add color control highlight, button normal, activated -->

        <!-- Customize your theme here. -->
    </style>

    <style name="ToolbarTheme" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="android:textColor">#fff</item>
    </style>
    <style name="TextStyle" parent="AppTheme">
        <item name="android:textSize">14sp</item>
    </style>
    <style name="WYQTextRegular" parent="TextAppearance.AppCompat">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="WYQTextBold" parent="TextAppearance.AppCompat">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@android:color/white</item>
    </style><style name="WYQRadioButton" parent="TextAppearance.AppCompat">
    <item name="android:textSize">16sp</item>
    <item name="android:textColor">@android:color/white</item>
    </style>
    <style name="WYQMainButton">
        <item name="android:background">@drawable/main_button_background</item>
        <item name="android:textColor">@drawable/main_button_text_color</item>
    </style>

</resources>
